  <!-- Start features section -->
  <section id="mu-features">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 col-md-12">
          <div class="mu-features-area">
            <!-- Start Title -->
            <div class="mu-title">
              <h2>Order:</h2>
              <p>Tailor your order according to your needs, we calculate the price and you can proceed with our secure checkout methods which offer you buyer protection. If you have an order with specifications which are not offered in the form below, feel free to contact us at <em>info@ibessays.net</em> to start your order!</p>
            </div>
            <!-- End Title -->
            <div class="container" style="width: 70%">
            <form action="">
              <div class="form-group">
                <label for="orderType">Type</label>
                <select class="form-control" id="orderType">
                  <option>Internal Assesment</option>
                  <option>Extended Essay</option>
                  <option>Structure</option>
                </select>
              </div>
              <div class="form-group">
                <label for="orderSubject">Subject</label>
                <select class="form-control" id="orderSubject">
                  <option selected>Theory of Knowledge</option>
                  <option>Business and Management</option>
                  <option>Computer Science</option>
                  <option>Mathematics</option>
                  <option>English A</option>
                  <option>English B</option>
                  <option>Economics</option>
                  <option>Biology</option>
                  <option>Chemistry</option>
                  <option>Arts</option>
                  <option>Environmental Studies</option>
                  <option>History</option>
                  <option>Geography </option>
                  <option>Psychology </option>
                  <option>Information and Technology in Global Society(ITGS)</option>
                  <option>Physics</option>
                  <option>German B</option>
                  <option>Bosnian A</option>
                </select>
              </div>
              <div class="form-group">
                <label for="orderLevel">Level</label>
                <select class="form-control" id="orderLevel">
                  <option>HL</option>
                  <option selected>SL</option>
                  <option>Studies</option>
                </select>
              </div>
              <div class="form-group">
                  <label for="orderDate">Deadline</label>
                  <select class="form-control" id="orderDate">
                    <option>1 Day</option>
                    <option>2 Days</option>
                    <option>4 Days</option>
                    <option selected>7 Days</option>
                    <option>10 Days</option>
                    <option>14 Days</option>
                  </select>
                </div>
              <div class="form-group">
                <label for="orderTitle">Title</label>
                <textarea class="form-control" id="orderTitle" rows="1"></textarea>
              </div>
              <div class="form-group">
                <label for="orderSpecifications">Specifications</label>
                <textarea class="form-control" id="orderSpecifications" rows="4"></textarea>
              </div>
              <!-- Cijena Ordera -->
              <div class="form-group">
                <label for="orderPrice">Price ($)</label>
                <input type="text" name="" id="orderPrice" class="form-control" readonly>
              </div>
              <!-- Kraj Cijene Ordera XDDD -->
              <button class="btn btn-primary" type="submit">Submit Order</button>
            </form>
          </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- End features section -->

